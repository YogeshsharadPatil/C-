#include<iostream>
using namespace std;

class Complex{
private:
	int real;
	int imag;
public:
	Complex( void ){
		this->real = 0;
		this->imag = 0;
	}
};
int main( void ){
	Complex c1;
	return 0;
}
int main1( void ){
	int num1 = 10;
	int num2 = num1;	//Shallow Copy
	cout << num1 <<endl;
	cout << num2 <<endl;
	return 0;
}
