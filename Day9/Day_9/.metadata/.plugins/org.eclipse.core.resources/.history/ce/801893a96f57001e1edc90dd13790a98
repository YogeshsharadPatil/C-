#include<iostream>
using namespace std;

class A{
private:
	int num1;
public:
	A( void ){
		this->num1 = 10;
	}
};
class B{
private:
	int num2;
public:
	B( void ){
		this->num2 = 20;
	}
};
void print( ){
	A a;
	cout << "Num1	:	" << a.num1 << endl;
	B b;
	cout << "Num2	:	" << b.num2 << endl;
}
int main( void ){
	//Test t;
	//t.print( );	//Not OK

	print( );
	return 0;
}

