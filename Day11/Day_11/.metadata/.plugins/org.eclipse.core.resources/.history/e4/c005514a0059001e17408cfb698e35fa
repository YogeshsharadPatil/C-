#include<iostream>
using namespace std;

void swap_object( char &object1, char &object2 ){
	int temp = object1;
	object1 = object2;
	object2 = temp;
}

void swap_object( int &object1, int &object2 ){
	int temp = object1;
	object1 = object2;
	object2 = temp;
}

int main( void ){
	char ch1 = 'A';
	char ch2 = 'B';

	swap_object( ch1, ch2 );

	cout << "ch1	:	" << ch1 << endl;
	cout << "ch2	:	" << ch2 << endl;


	int a = 10;
	int b = 20;

	swap_object( a, b );

	cout << "a	:	" << a << endl;
	cout << "a	:	" << b << endl;



	return 0;
}
