#include<iostream>
using namespace std;

class Complex{
private:
	int real;
	int imag:
public:
	Complex( void ){
		this->real = 0;
		this->imag = 0;
	}
	Complex( int real, int imag ){
		this->real = real;
		this->imag = imag;
	}
};
int main( void ){
	Complex c1;
	c1.acceptRecord( );
	c1.printRecord( );
	return 0;
}
