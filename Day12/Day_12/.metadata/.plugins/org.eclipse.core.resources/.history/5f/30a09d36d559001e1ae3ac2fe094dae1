
class Employee : public Person{
private:
	int empid;
	float salary;
public:
	Employee( );
	Employee( string name, int age, int empid, float salary ) : Person( name, age ){
		this->empid = empid;
		this->salary = salary;
	}
};
int main( void ){
	Employee emp( "Sandeep" , 40, 3778, 45000.50f );
	return 0;
}
