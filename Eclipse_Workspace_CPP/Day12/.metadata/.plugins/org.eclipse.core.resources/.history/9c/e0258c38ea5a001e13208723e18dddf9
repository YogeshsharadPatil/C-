#include<iostream>
using namespace std;
class Rectangle{
protected:
	int length;
	int breadth;
	int area;
public:
	Rectangle(){
		this->length = 1;
		this->breadth = 1;
		this->area = 1;
	}
	int calculate(){
		return this->area = this->length * this->breadth;
	}
	void acceptRecord(){
		cin>>this->length;
		cin>>this->breadth;
	}
	void printRecord(){
		cout<<this->length;
		cout<<this->area;
		cout<<this->calculate();
	}
};
class Circle{
protected:
	double radius;
	double area;
public:
	Circle(){
		this->radius = 1;
		this->area=1;
	}
	void acceptRecord(){
		cin>>this->radius;
	}
	double calculate(){
		return (3.14 * this->radius * this->radius);
	}
	void printRecord(){
		cout<<this->calculate();

	}
};

int main(){
	return 0;
}
