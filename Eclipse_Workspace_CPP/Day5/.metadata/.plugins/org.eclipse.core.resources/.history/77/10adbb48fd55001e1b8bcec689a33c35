#include<iostream>
using namespace std;
class Complex{
	int real;
	int img;
public:
	Complex(){
		this->real = 0;
		this->img = 0;
	}
	Complex(int real){
		this->real = real;{
	}
    COmplex(int real , int img){
    	this->real = real;
    	this->img = img;
    }
    void printRecord(){
    	cout<<this->real;
    	cout<<this->img;
    }

	}
};

int main(){
	Complex c1;
}



