#include<iostream>
using namespace std;

void accept_record( int &number ){
	cout << "Number	:	";
	cin >> number;
}
void print_record( int &result ){
	cout << "Result	:	" << result << endl;
}
int main( void ){
	cout <<"File/Connection Open"<<endl;

	int num1;
	accept_record( num1 );

	int num2;
	accept_record( num2 );

	int result = num1 / num2;
	print_record( result );

	cout <<"File/Connection Close"<<endl;
	return 0;
}
