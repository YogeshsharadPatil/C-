#include<iostream>
using namespace std;

namespace nutil{
	class Complex{
	private:
		int real;
		int imag;
	public:
		Complex( void ){
			this->real = 0;
			this->imag = 0;
		}
		Complex( int real, int imag ){
			this->real = real;
			this->imag = imag;
		}
	};
}
int main( void ){
	using namespace nutil;
	Complex c1(10,20);

	return 0;
}
